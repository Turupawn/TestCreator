<p id="notice"><%= notice %></p>

<h1>Listing Tests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.name %></td>
        <td><a href="/test_answers/new?test_id=<%=test.id%>">Tomar</a></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
